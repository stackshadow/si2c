<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="softi2c" />
		<Option makefile="si2c.make" />
		<Option makefile_is_custom="1" />
		<Option execution_dir="../../si2c" />
		<Option pch_mode="2" />
		<Option compiler="avr-gcc" />
		<Build>
			<Target title="Release">
				<Option type="4" />
				<Option compiler="avr-gcc" />
				<Compiler>
					<Add option="-mmcu=atmega8" />
					<Add option="-O" />
					<Add option="-print-file-name=include" />
				</Compiler>
			</Target>
			<Target title="Interrupt">
				<Option type="4" />
				<Option compiler="avr-gcc" />
				<Compiler>
					<Add option="-mmcu=atmega8" />
					<Add option="-O" />
					<Add option="-print-file-name=include" />
				</Compiler>
			</Target>
			<Target title="Upload">
				<Option type="4" />
				<Option compiler="avr-gcc" />
			</Target>
			<Target title="Preprocess">
				<Option type="4" />
				<Option compiler="avr-gcc" />
				<Compiler>
					<Add option="-mmcu=atmega8" />
					<Add option="-O" />
					<Add option="-print-file-name=include" />
				</Compiler>
			</Target>
			<Target title="docs">
				<Option type="4" />
				<Option compiler="avr-gcc" />
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="Interrupt and Upload" targets="Upload;Interrupt;" />
			<Add alias="Release and Upload" targets="Upload;Release;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Unit filename="../doc/si2c/source/doxyfile">
			<Option target="Release" />
			<Option target="Interrupt" />
		</Unit>
		<Unit filename="../doc/si2c/source/mainpage.h">
			<Option target="Release" />
			<Option target="Interrupt" />
		</Unit>
		<Unit filename="../lib/si2c/si2c.c">
			<Option compilerVar="CC" />
			<Option target="Release" />
			<Option target="Interrupt" />
		</Unit>
		<Unit filename="../lib/si2c/si2c.h">
			<Option target="Release" />
			<Option target="Interrupt" />
		</Unit>
		<Unit filename="../lib/si2c/si2cAdditional.S" />
		<Unit filename="../lib/si2c/si2cSCL.S" />
		<Unit filename="../lib/si2c/si2cSlave.S" />
